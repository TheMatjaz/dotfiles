# .gitignore_global
# This gitignore is globally applied to any repository on this machine

# This allows to use this same file also for hg
syntax: glob
    
# The gitignore file of any repository
.gitignore

# Temporary backup files from text editors and Emacs
*~
.#*#

# OS X files
.DS_Store

# Windows files
Thumbs.db

# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]

# C extensions
*.so

# PyCharm editor
.idea

# Distribution / packaging
.Python
target/
env/
build/
develop-eggs/
dist/
downloads/
eggs/
lib/
lib64/
parts/
sdist/
var/
*.egg-info/
.installed.cfg
*.egg

# PyInstaller
# Usually these files are written by a python script from a template
# before PyInstaller builds the exe, so as to inject date/other infos into it.
*.manifest
*.spec

# Installer logs
pip-log.txt
pip-delete-this-directory.txt

# Unit test / coverage reports
htmlcov/
.tox/
.coverage
.cache
nosetests.xml
coverage.xml

# Translations
*.mo
*.pot

# Logs
*.log
wget-log*
